<?xml version="1.0" encoding="utf-8" ?>
<nlog xmlns="http://www.nlog-project.org/schemas/NLog.xsd" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
  <targets>
    <target name="console" xsi:type="Console" layout="[${logger}] [${level:uppercase=true}] : ${message}"/>
    <target name="logFile" xsi:type="File" fileName="C:\Users\aprogrammer\Documents\Visual Studio 2013\Projects\AkkaESSample\SampleWeb\web_test_log.txt" layout="[${longdate}] [${logger}] [${level:uppercase=true}] : ${message}"/>
  </targets>
  <rules>
<!--    <logger name="*" minlevel="Debug" writeTo="console"/>-->
    <logger name="*" minlevel="Debug" writeTo="logFile"/>
  </rules>
</nlog>
